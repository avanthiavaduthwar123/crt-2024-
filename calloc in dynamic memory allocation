#include <stdio.h>
#include<stdlib.h>
int main() {
    // Write C code here
    int *a;
    a=(int*)calloc(5,sizeof(int));
    //a=(int*)malloc(5*sizeof(int));
    int i;
    for(i=0;i<5;i++)
    {
        scanf("%d",(a+i));
        
    }
    for(i=0;i<5;i++)
    {
        printf("%d address=%u\n",*(a+i),(a+i));
    }
    

    return 0;
}


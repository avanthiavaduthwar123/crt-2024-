#include <stdio.h>

int main() {
    // Write C code here
    int a;
    scanf("%d",&a);
    if(a>0)
    {
        printf("positive no");
    }
    else if(a<0)
    {
        printf("negative no");
    }
else
{
    printf("zero");
}
    return 0;
}

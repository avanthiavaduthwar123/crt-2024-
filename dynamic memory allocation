#include <stdio.h>
#include<stdlib.h>

int main() {
    // Write C code here
    int *a,*b,*c;
    a=(int*)malloc(sizeof(int));
    b=(int*)malloc(sizeof(int));
    scanf("%d%d",a,b);
    c=(int*)malloc(sizeof(int));
    *c=*a+*b;
    free(a);
    free(b);
    printf("%d",*c);
    free(c);
    
    

    return 0;
}

